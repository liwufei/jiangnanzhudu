{
	"name": "laravelvip/laravelvip",
	"description": "The B2b2c Mall based on Laravel.",
	"keywords": [
		"b2b2c",
		"mall",
		"laravel"
	],
	"license": "MIT",
	"type": "project",
	"require": {
		"php": ">=8.0",
		"alibabacloud/live": "^1.8",
		"aliyunmq/mq-http-sdk": "^1.0",
		"barryvdh/laravel-debugbar": "^3.15",
		"doctrine/dbal": "^4.2",
		"elasticsearch/elasticsearch": "^8.18",
		"firebase/php-jwt": "^6.11",
		"getuilaboratory/getui-pushapi-php-client-v2": "dev-master",
		"gregwar/captcha": "^1.3",
		"ifsnop/mysqldump-php": "^2.11",
		"jaeger/querylist": "^4.4",
		"jpush/jpush": "^3.7",
		"laravel/framework": "^10.48",
		"laravel/helpers": "^1.7",
		"laravel/horizon": "^5.33",
		"laravel/legacy-factories": "^1.4",
		"laravel/octane": "^2.10",
		"laravel/sanctum": "^4.1",
		"laravel/ui": "^4.6",
		"laravelvip/kdniao": "^1.0",
		"luoyy/ali-oss-storage": "^4.0",
		"maatwebsite/excel": "^3.1",
		"orhanerday/open-ai": "^5.3",
		"overtrue/easy-sms": "^3.1",
		"overtrue/laravel-pinyin": "^5.2",
		"overtrue/socialite": "^4.11",
		"phpoffice/phpspreadsheet": "^4.4",
		"predis/predis": "^3.0",
		"pusher/pusher-php-server": "^7.2",
		"simplesoftwareio/simple-qrcode": "^4.2",
		"w7corp/easywechat": "^6.17",
		"workerman/gateway-worker": "^4.0",
		"workerman/workerman": "^5.1",
		"yansongda/pay": "^3.7"
	},
	"require-dev": {
		"brianium/paratest": "^7.10",
		"filp/whoops": "^2.18",
		"mockery/mockery": "^1.6",
		"nunomaduro/collision": "^8.8",
		"phpunit/phpunit": "^9.6",
		"symfony/thanks": "^1.4"
	},
	"autoload": {
		"classmap": [
			"database/seeds",
			"database/factories"
		],
		"psr-4": {
			"App\\": "app/"
		},
		"files": [
			"app/Support/constant.php",
			"app/Support/helpers.php",
			"app/Support/frontend_helpers.php",
			"app/Support/backend_helpers.php",
			"app/Support/seller_helpers.php",
			"app/Support/store_helpers.php",
			"app/Support/trade_helpers.php",
			"app/Support/statistic_helpers.php"
		]
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\": "tests/"
		}
	},
	"extra": {
		"laravel": {
			"dont-discover": [
			]
		}
	},
	"scripts": {
		"post-root-package-install": [
			"@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
		],
		"post-create-project-cmd": [
			"@php artisan key:generate"
		],
		"post-autoload-dump": [
			"Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
			"@php artisan package:discover"
		]
	},
	"config": {
		"preferred-install": "dist",
		"sort-packages": true,
		"optimize-autoloader": true,
		"allow-plugins": {
			"easywechat-composer/easywechat-composer": true,
			"symfony/thanks": true
		}
	},
	"minimum-stability": "dev",
	"prefer-stable": true
}
